<script> 
  var opr=prompt("operator : ");
var num1 =prompt("Number 1: ");
  var num2= prompt ("Number 2: ");

if(opr=="+");
{
console.log (num1 +num2)
};
  else if(opr =="*")
{console.log(num1 * num2)};
  else if (opr == "-" );
    {console.log(num1 - num2)};
  else if(opr =="/");
    {console.log(num1 /num2)};
</script>
